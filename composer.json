{
    "name": "redfiredigital/helper",
    "description": "Package with common functionality",
    "type": "library",
    "require": {
        "aws/aws-sdk-php": "^3.257",
        "endroid/qr-code-bundle": "^4.0@dev",
        "imagine/imagine": "^1.2",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/console": ">=5.*-dev",
        "symfony/dependency-injection": ">=5.*-dev",
        "symfony/dotenv": ">=5.*-dev",
        "symfony/finder": ">=5.*-dev",
        "symfony/runtime": "^6.2",
        "aruljohn/popular-baby-names": "1.0.0",
        "hippy2094/random-name-generator": "*"
    },
    "autoload": {
        "psr-4": {
            "RedFireDigital\\Helper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Graham Owens",
            "email": "gra@redfiredigital.uk"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin",
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true
        }
    },

    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^10.1@dev"
    },
    "repositories":[
        {
            "type": "package",
            "package": {
                "name": "aruljohn/popular-baby-names",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/aruljohn/popular-baby-names.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "hippy2094/random-name-generator",
                "version": "1.0.0",
                "source": {
                    "url": "git@github.com:hippy2094/random-name-generator.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
